{
  "name": "synthetix-subgraph",
  "license": "MIT",
  "version": "1.2.1",
  "author": "Synthetix",
  "main": "index.js",
  "browser": "index.min.js",
  "bin": {
    "snxData": "bin.js"
  },
  "scripts": {
    "lint": "eslint .",
    "create-yaml-ovm": "node mustache/create-yaml create-yaml --env mainnet-ovm | mustache - mustache/yaml.mustache > subgraph-ovm.yaml",
    "create-yaml-kovan-ovm": "node mustache/create-yaml create-yaml --env kovan-ovm | mustache - mustache/yaml.mustache > subgraph-kovan-ovm.yaml",
    "codegen": "node prepare-abis && npm run create-yaml-ovm && graph codegen -- subgraph-ovm.yaml",
    "codegen:kovan-ovm": "node prepare-abis && npm run create-yaml-kovan-ovm && graph codegen -- subgraph-kovan-ovm.yaml",
    "build": "graph build subgraph-ovm.yaml",
    "build:kovan-ovm": "graph build subgraph-kovan-ovm.yaml",
    "deploy-helper": "graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ --access-token $THE_GRAPH_SNX_ACCESS_TOKEN",
    "deploy": "npm run deploy-helper -- synthetixio-team/synthetix-ovm subgraph-ovm.yaml",
    "deploy:kovan-ovm": "npm run deploy-helper -- synthetixio-team/synthetix-kovan-ovm subgraph-kovan-ovm.yaml"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "git://github.com/graphprotocol/graph-cli.git#ebeeba0009f026bacf940f25ee7b875fe7676032",
    "@graphprotocol/graph-ts": "0.18.1",
    "chalk": "2.4.2",
    "commander": "2.20.0",
    "mustache": "4.1.0",
    "synthetix": "2.40.1-alpha"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "2.28.0",
    "@typescript-eslint/parser": "2.28.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-import-resolver-typescript": "1.1.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.0",
    "minify": "4.1.3",
    "prettier": "1.17.1",
    "typescript": "3.8.3"
  }
}
