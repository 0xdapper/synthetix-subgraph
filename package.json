{
  "name": "synthetix-subgraph",
  "license": "MIT",
  "version": "1.2.1",
  "author": "Synthetix",
  "main": "index.js",
  "browser": "index.min.js",
  "bin": {
    "snxData": "bin.js"
  },
  "scripts": {
    "lint": "eslint .",
    "pre-codegen-synthetix": "node ./setup init-subgraph --subgraph synthetix",
    "codegen:synthetix": "node prepare-abis && npm run pre-codegen-synthetix && graph codegen -- subgraphs/synthetix/autoGeneratedYaml/synthetix-mainnet.yaml && graph codegen -- subgraphs/synthetix/autoGeneratedYaml/synthetix-kovan.yaml && graph codegen -- subgraphs/synthetix/autoGeneratedYaml/synthetix-optimism.yaml && graph codegen -- subgraphs/synthetix/autoGeneratedYaml/synthetix-optimism-kovan.yaml",
    "build:synthetix": "graph build subgraphs/synthetix/autoGeneratedYaml/synthetix-mainnet.yaml && graph build subgraphs/synthetix/autoGeneratedYaml/synthetix-kovan.yaml && graph build subgraphs/synthetix/autoGeneratedYaml/synthetix-optimism.yaml && graph build subgraphs/synthetix/autoGeneratedYaml/synthetix-optimism-kovan.yaml",
    "deploy-helper": "graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ --access-token $THEGRAPH_SNX_ACCESS_TOKEN",
    "deploy:synthetix-mainnet": "npm run deploy-helper -- synthetixio-team/synthetix subgraphs/synthetix/autoGeneratedYaml/synthetix-mainnet.yaml",
    "deploy:synthetix-kovan": "npm run deploy-helper -- synthetixio-team/synthetix-kovan subgraphs/synthetix/autoGeneratedYaml/synthetix-kovan.yaml",
    "deploy:synthetix-optimism": "npm run deploy-helper -- synthetixio-team/synthetix-optimism subgraphs/synthetix/autoGeneratedYaml/synthetix-optimism.yaml",
    "deploy:synthetix-optimism-kovan": "npm run deploy-helper -- synthetixio-team/synthetix-optimism-kovan subgraphs/synthetix/autoGeneratedYaml/synthetix-optimism-kovan.yaml",
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "git://github.com/graphprotocol/graph-cli.git#ebeeba0009f026bacf940f25ee7b875fe7676032",
    "@graphprotocol/graph-ts": "0.18.1",
    "chalk": "2.4.2",
    "commander": "2.20.0",
    "mustache": "4.1.0",
    "synthetix": "2.40.1-alpha"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "2.28.0",
    "@typescript-eslint/parser": "2.28.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-import-resolver-typescript": "1.1.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.0",
    "minify": "4.1.3",
    "node-fetch": "2.6.1",
    "prettier": "1.17.1",
    "typescript": "3.8.3"
  }
}
